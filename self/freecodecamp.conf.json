      "/.logs/.temp.log",
      "config/",
      "node_modules/",
      ".git/",
      "target/",
      ".vscode/",
      "freecodecamp.conf.json"
    ]
  },
  "tooling": {
    "helpers": "./tooling/helpers.js",
    "plugins": "./tooling/plugins.js"
