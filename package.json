{
  "name": "@freecodecamp/freecodecamp-os",
  "author": "freeCodeCamp",
  "version": "1.5.4",
  "description": "Package used for freeCodeCamp projects with the freeCodeCamp Courses VSCode extension",
  "scripts": {
    "start": "npm run build:client && node ./.freeCodeCamp/tooling/server.js",
    "develop": "npm run develop:client & npm run develop:server",
    "build:git": "node ./.freeCodeCamp/tooling/git/build.js",
    "build:client": "NODE_ENV=production webpack --config ./.freeCodeCamp/webpack.config.cjs",
    "develop:client": "NODE_ENV=development webpack --mode development",
    "develop:server": "nodemon --watch ./dist/ --watch ./tooling/ --ignore ./config/ ./tooling/server.js",
    "postinstall": "npm run build:client && node ./.freeCodeCamp/npm/postinstall.js",
    "uninstall": "node ./.freeCodeCamp/npm/uninstall.js"
  },
  "dependencies": {
    "@babel/core": "7.18.10",
    "@babel/plugin-syntax-import-assertions": "7.18.6",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@types/marked": "4.0.6",
    "@types/node": "^18.7.15",
    "@types/prismjs": "1.26.0",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "babel-loader": "8.2.5",
    "babel-plugin-prismjs": "2.1.0",
    "chai": "4.3.6",
    "chokidar": "3.5.3",
    "css-loader": "6.7.1",
    "dotenv-webpack": "8.0.1",
    "express": "4.18.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "logover": "1.3.1",
    "marked": "4.1.0",
    "nodemon": "2.0.19",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "prismjs": "1.29.0",
    "process": "0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.1",
    "typescript": "4.8.2",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freeCodeCamp/freeCodeCampOS"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "type": "module"
}
